generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Expense {
  id          String   @id @default(cuid())
  clerkUserId String
  date        DateTime
  description String
  category    String
  amount      Float
}

model Budget {
  id          String   @id @default(cuid())
  clerkUserId String
  category    String
  limit       Float
}
